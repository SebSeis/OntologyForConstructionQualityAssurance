     xml:base="https://w3id.org/digitalconstruction/0.5/Processes"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dice="https://w3id.org/digitalconstruction/0.5/Entities#"
     xmlns:dicv="https://w3id.org/digitalconstruction/0.5/Variables#"
     xmlns:obda="https://w3id.org/obda/vocabulary#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="https://w3id.org/digitalconstruction/0.5/Processes">
        <owl:imports rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities/entities.ttl"/>
        <dc:creator>Seppo Törmä</dc:creator>
        <dc:creator>Yuan Zheng</dc:creator>
        <dc:date>2021-04-15</dc:date>
        <dc:title>Digital Construction Processes</dc:title>
        <terms:abstract>Processes ontology defines the concepts and relations for activities and resources. </terms:abstract>
        <terms:created>2019-12-20</terms:created>
        <terms:description>Process ontology for digital construction</terms:description>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <vann:preferredNamespacePrefix>dicp</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/digitalconstruction/0.5/Processes</vann:preferredNamespaceUri>
        <rdfs:label>Digital Construction Processes</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://w3id.org/digitalconstruction/0.5"/>
    </owl:Ontology>
    


