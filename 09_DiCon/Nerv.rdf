    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#actsOnProperty -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#actsOnProperty">
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Actuation"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Variables#Property"/>
        <rdfs:label>actsOnProperty</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#actuates -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#actuates">
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isActuatedBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#MaterialEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Actuation"/>
        <rdfs:label>actuates</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasAssignedResource -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasAssignedResource">
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isResourceIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#ResourceRole"/>
        <rdfs:comment>The resource role that is assigned to this activity.</rdfs:comment>
        <rdfs:label>hasAssignedResource</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasCondition -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasCondition">
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isConditionOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Variables#Constraint"/>
        <rdfs:comment>The constraint that has to be in effect at the start of the activity. E.g., the agent has to be in the location of the activity</rdfs:comment>
        <rdfs:label>hasCondition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasCostUnit -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasCostUnit">
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Resource"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Variables#Unit"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasEffect -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasEffect">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isEffectOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:comment>The constraint that has to be in effect in the end of the activity. E.g., the moved item has to be in the target location of the activity</rdfs:comment>
        <rdfs:label>hasEffect</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasFinalLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasFinalLocation">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#hasLocation"/>
        <rdfs:label>hasFinalLocation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasInitialLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasInitialLocation">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#hasLocation"/>
        <rdfs:label>hasInitialLocation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasInputObject -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasInputObject">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#hasObject"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isInputObjectIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#Entity"/>
        <rdfs:comment>The subproperty of :hasObject. An object that an activity has in the beginning but not anymore in the end. For example, demolition has as its input object a building.</rdfs:comment>
        <rdfs:label>hasInputObject</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasLocation">
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#Location"/>
        <rdfs:comment>The location in which the activity happens. Can be a trajectory if the activity does not stay in the same location</rdfs:comment>
        <rdfs:label>hasLocation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasLocationPhase -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasLocationPhase">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#hasSubActivity"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isLocationPhaseOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:comment>A subproperty of :hasSubActivity. A subactivity that corresponds to a location-specific phase. </rdfs:comment>
        <rdfs:label>hasLocationPhase</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasObject -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasObject">
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isObjectIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#Entity"/>
        <rdfs:comment>The object that an activity acts on or is focused to. For example, a transportation activity can have as its object a container, or painting can have as its object a wall.</rdfs:comment>
        <rdfs:label>hasObject</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasObjectPhase -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasObjectPhase">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#hasSubActivity"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isObjectPhaseOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:comment>A subproperty of :hasSubActivity. A subactivity that corresponds to an object-specific phase. </rdfs:comment>
        <rdfs:label>hasObjectPhase</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasObservedProperty -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasObservedProperty">
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Observation"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Variables#Property"/>
        <rdfs:label>hasObservedProperty</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasObservedResult -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasObservedResult">
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Observation"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Variables#QuantitativeState"/>
        <rdfs:label>hasObservedResult</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasOutputObject -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasOutputObject">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#hasObject"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isOutputObjectIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#Entity"/>
        <rdfs:comment>The subproperty of :hasObject. An object that an activity has in the end even though it didn&apos;t have it in the beginning. For example, procurement has its output object a purchased product.</rdfs:comment>
        <rdfs:label>hasOutputObject</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasProcessPhase -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasProcessPhase">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#hasSubActivity"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isProcessPhaseOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:comment>A subproperty of :hasSubActivity. A subactivity that corresponds to a phase in the process. </rdfs:comment>
        <rdfs:label>hasProcessPhase</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasProductionRate -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasProductionRate">
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#CapabilityToWork"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#ProductionRate"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#hasSubActivity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#hasSubActivity">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#hasProperOccurrentPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isSubActivityOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:label>hasSubActivity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isActuatedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isActuatedBy">
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Actuation"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#MaterialEntity"/>
        <rdfs:label>isActuatedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isConditionOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isConditionOf">
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Variables#Constraint"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:label>isConditionOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isEffectOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isEffectOf">
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Variables#Constraint"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:label>isEffectOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isInputObjectIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isInputObjectIn">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isObjectIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:comment>The subproperty of :isObjectIn. An activity that has the object in the beginning but not anymore in the end. For example, building is the input object in demolition.</rdfs:comment>
        <rdfs:label>isInputObjectIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isLocationPhaseOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isLocationPhaseOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isSubActivityOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:comment>A subproperty of :isSubActivityOf. A activity whose location-specific phase this activity is.</rdfs:comment>
        <rdfs:label>isLocationPhaseOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isNeededByActivity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isNeededByActivity">
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#needsCapability"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#Capability"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:comment>The activity that needs a capability from some of its ingredients</rdfs:comment>
        <rdfs:label>isNeededByActivity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isObjectIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isObjectIn">
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:comment>The activity that acts on the object. For example, a shipment is the object of transportation.</rdfs:comment>
        <rdfs:label>isObjectIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isObjectPhaseOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isObjectPhaseOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isSubActivityOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:comment>A subproperty of :isSubActivityOf. A activity whose object-specific phase this activity is.</rdfs:comment>
        <rdfs:label>isObjectPhaseOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isObservedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isObservedBy">
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#observes"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Observation"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#MaterialEntity"/>
        <rdfs:label>isObservedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isOccupiedByActivity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isOccupiedByActivity">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#occupiesTemporalRegion"/>
        <owl:inverseOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#occupiesTimeInterval"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#TimeInterval"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:label>isOccupiedByActivity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isOutputObjectIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isOutputObjectIn">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isObjectIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:comment>The subproperty :isObjectIn. An activity that has the object in the end even though it didn&apos;t have it in the beginning. For example, a purchased product is an output object of procurement.</rdfs:comment>
        <rdfs:label>isOutputObjectIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isProcessPhaseOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isProcessPhaseOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#isSubActivityOf"/>
        <rdfs:comment>The subproperty of :isSubActivityOf.</rdfs:comment>
        <rdfs:label>isProcessPhaseOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isResourceIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isResourceIn">
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#ResourceRole"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:comment>The activity in whose execution this entity plays a resource role .</rdfs:comment>
        <rdfs:label>isResourceIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#isSubActivityOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#isSubActivityOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#properOccurrentPartOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:label>isSubActivityOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#needsCapability -->

    <owl:ObjectProperty rdf:about="https://w3id.org/digitalconstruction/0.5/Processes#needsCapability">
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/0.5/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/0.5/Entities#Capability"/>
        <rdfs:comment>A capability that some ingredient of an activity needs to provided to make the execution of the activity possible.</rdfs:comment>
        <rdfs:label>needsCapability</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/digitalconstruction/0.5/Processes#observes -->

